upstream backend{
 server 13.213.71.0:80 max_fails=3 fail_timeout=30s;
 server 13.250.42.91:80 max_fails=3 fail_timeout=30s;
}
Nếu failed 3 lần liên tiếp trong 30s sẽ ko phân phối traffic trong 30s

server {
	listen 80 default_server;

	server_name _;

	location / {
	 	proxy_pass http://backend;
	}

}
